{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  data() {\n    return {\n      repairServices: [],\n      currentPage: 1,\n      totalPages: 0,\n      pageSize: 10,\n      backendUrl: 'http://localhost:3000'\n    };\n  },\n  async created() {\n    await this.fetchRepairServices();\n  },\n  methods: {\n    async fetchRepairServices() {\n      try {\n        const response = await fetch(`${this.backendUrl}/api/repair-services?page=${this.currentPage}`, {\n          method: 'GET',\n          credentials: 'include'\n        });\n        if (!response.ok) {\n          if (response.status === 401) {\n            this.$router.push('/login');\n            return;\n          }\n          throw new Error(`HTTP error! Status: ${response.status}`);\n        }\n        const data = await response.json();\n        this.repairServices = data.repairServices || [];\n        this.totalPages = data.totalPages || 0;\n        this.currentPage = data.currentPage || 1;\n      } catch (err) {\n        console.error('Ошибка при получении услуг ремонта:', err);\n        this.repairServices = [];\n        this.totalPages = 0;\n      }\n    },\n    async prevPage() {\n      if (this.currentPage > 1) {\n        this.currentPage--;\n        await this.fetchRepairServices();\n      }\n    },\n    async nextPage() {\n      if (this.currentPage < this.totalPages) {\n        this.currentPage++;\n        await this.fetchRepairServices();\n      }\n    },\n    getStatusClass(isAvailable) {\n      return isAvailable ? 'available' : 'unavailable';\n    }\n  }\n};","map":{"version":3,"names":["data","repairServices","currentPage","totalPages","pageSize","backendUrl","created","fetchRepairServices","methods","response","fetch","method","credentials","ok","status","$router","push","Error","json","err","console","error","prevPage","nextPage","getStatusClass","isAvailable"],"sources":["C:\\Users\\PK\\Desktop\\audio\\vue\\src\\components\\RepairServiceList.vue"],"sourcesCo